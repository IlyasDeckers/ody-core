    "nyholm/psr7-server": "^1.1",
    "psr/log": "^1.1 || ^2.0 || ^3.0",
    "php-di/php-di": "^7.0",
    "php-di/invoker": "^2.3",
    "symfony/console": "^7.2",
    "vlucas/phpdotenv": "^5.6",
